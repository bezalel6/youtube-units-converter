{
  "compilerOptions": {
    "outDir": "./dist/unpacked",
    "noImplicitAny": true,
    "module": "es2022",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "downlevelIteration": true,
    "strict": false,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}